import re


def isPalindrome(self, s: str) -> bool:
    #대소문자 구분 없애기 위해 lower 적용 
    str_l = str.lower()
    #띄어쓰기 제거 
    str_1 = str1.replace(" ","")
    #영문자, 숫자빼고 제거 
    str.re.sub("^A-Za-z0-9","",str_l)   
    #역순 배열하기
    str_rv = str_l[::-1]
    if str_rv == str_l:
        return True
    else:
        return False

